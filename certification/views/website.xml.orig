        <!-- Identification matériel -->
        <template id="portal_identification_equipment" name="Identification matériel">
            <link href="/certification/static/src/css/bootstrap.min.css" rel="stylesheet" type="text/css"/>

            <t t-call="website.layout">
                <div class="o_portal_my_purchases panel row">
                    <t t-if="not equipment and len(search) > 0">
                        <div class="alert alert-warning" role="alert">
                          Aucun matériel trouvé.
                        </div>
                    </t>
                    <!--<t t-if="not equipment and len(search_nfc) > 0">
                        <div class="alert alert-warning" role="alert">
                          Aucun matériel trouvé.
                        </div>
                    </t>-->

                    <input type="hidden" name="origin" id="origin" t-att-value="origin"></input>
                    <br/>
                    <div class="col-lg-12">
                        <form id="form_identification" role="form" method="post" t-att-action="'/my/identification'">
                            <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>

                             <!--identification materiel-->
                            <t t-if="origin != 'bo'">
                                <div class="panel panel-default col-xs-12 col-sm-12 col-md-4 col-lg-4">
                                 <center>
                                     <h4>Identification matériel</h4>
                                     <div class="col-lg-12">
<!--                                        <div id="nfc_img" class="nfc scan"><div></div></div>-->
                                        <!--<div id="message_nfc"></div>-->
                                    </div>

                                    <div class="col-lg-12">
                                        <div class="form-group">
                                            <label class="o_form_label oe_edit_only" for="num_materiel">Numéro de matériel</label>
                                            <input type="text" name="num_materiel" id="num_materiel" class="form-control" placeholder="N° de matériel"></input>
                                            <br/>
                                            <t t-if="origin == 'bo'">
                                              <a href="/web#view_type=list&amp;model=critt.equipment&amp;action=certification.act_equipement_view" class="btn btn-dark">Recherche avancée</a>
                                            </t>
                                            <t t-else="">
                                                <a href="/my/equipments" class="btn btn-dark">Recherche avancée</a>
                                            </t>
                                        </div>
                                    </div>
                                 </center>
                            </div>
                            </t>
                            <t t-else="">
                                <div class="panel panel-default col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                     <center>
                                         <h4>Identification matériel</h4>
                                         <div class="col-lg-12">
<!--                                            <div id="nfc_img" class="nfc scan"><div></div></div>-->
                                            <!--<div id="message_nfc"></div>-->
                                        </div>
                                     </center>
                                </div>
                            </t>




                            <!-- controles à planifier -->
                            <div t-if="origin != 'bo'" class="col-xs-12 col-sm-12 col-md-4 col-lg-4">
                                <center>
                                    <h4>Contrôles à planifier</h4>
                                     <table class="table table-striped" style="display:none" id="controls_to_plan">
                                        <thead>
                                          <tr>
                                            <th t-if="origin == 'bo'">Client</th>
                                            <th>N° matériel</th>
                                            <th>Délai</th>
                                          </tr>
                                        </thead>
                                        <tbody>
                                            <tr t-foreach="controls_to_plan" t-as="equip">
                                                <td t-if="origin == 'bo'">
                                                    <!--<t t-esc="equip.owner_user_id.company_name"></t>-->
                                                    <a t-att-href="('/web#id=%s&amp;view_type=form&amp;model=res.partner&amp;action=certification.act_client_view' % equip.owner_user_id.partner_id.id)" t-esc="equip.owner_user_id.company_name"></a>
                                                </td>
                                                <td>
                                                    <t t-if="origin == 'bo'">
                                                      <a t-att-href="('/web#id=%s&amp;view_type=form&amp;model=critt.equipment&amp;action=certification.act_equipement_view' % equip.id)" t-esc="equip.num_materiel"></a>
                                                    </t>
                                                    <t t-else="">
                                                      <a t-att-href="('/my/equipment/%s' % equip.id)" t-esc="equip.num_materiel"></a>
                                                    </t>
                                                </td>
                                                <td class="controls_to_plan_date">
                                                     <t t-esc="equip.audit_suivant" t-options='{"widget": "date"}'></t>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </center>
                            </div>

                            <!-- prochains controles -->
                            <div t-if="origin != 'bo'" class="col-xs-12 col-sm-12 col-md-4 col-lg-4">
                                <center>
                                    <h4>Prochains contrôles</h4>
                                    <table class="table table-striped" style="display:none" id="next_controls">
                                        <thead>
                                          <tr>
                                            <th t-if="origin == 'bo'">Client</th>
                                            <th>N° matériel</th>
                                            <th>Date</th>
                                          </tr>
                                        </thead>
                                        <tbody>
                                            <tr t-foreach="next_controls" t-as="equip">
                                                <td t-if="origin == 'bo'">
                                                    <!--<t t-esc="equip.owner_user_id.company_name"></t>-->
                                                    <a t-att-href="('/web#id=%s&amp;view_type=form&amp;model=res.partner&amp;action=certification.act_client_view' % equip.owner_user_id.partner_id.id)" t-esc="equip.owner_user_id.company_name"></a>
                                                </td>
                                                <td>
                                                    <t t-if="origin == 'bo'">
                                                      <a t-att-href="('/web#id=%s&amp;view_type=form&amp;model=critt.equipment&amp;action=certification.act_equipement_view' % equip.id)" t-esc="equip.num_materiel"></a>
                                                    </t>
                                                    <t t-else="">
                                                      <a t-att-href="('/my/equipment/%s' % equip.id)" t-esc="equip.num_materiel"></a>
                                                    </t>
                                                </td>
                                                <td>

                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </center>
                            </div>

                            </form>
                    </div>

                    <br/>
                    <!-- Devis acceptés -->
                    <!-- prochains controles -->
                        <div t-if="origin != 'bo'" class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                            <center>
                                <h4>
                                    <t t-if="origin == 'bo'">Devis acceptés</t>
