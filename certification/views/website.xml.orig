                                      <tr>
                                        <!--<th>Photo</th>-->
                                        <th>N° Matériel</th>
                                        <th>N° devis</th>
                                        <th>Date</th>
                                        <th>N° commande</th>
                                        <th t-if="origin == 'bo'">Validé par le client</th>
                                      </tr>
                                    </thead>
                                    <tbody>
                                        <tr t-foreach="quotes" t-as="line">
                                             <!--<td>
												<t t-foreach="line.order_line_equipment" t-as="equipment">
                                                    <img class="zoom" t-if="equipment.equipment_id.image_photo" width="70" t-att-src="('data:image/jpeg;base64,%s' % equipment.equipment_id.image_photo)"></img>
                                                    <br/>
                                                </t>
                                            </td>-->
                                            <td>
                                                <t t-if="origin == 'bo'">
                                                    <t t-foreach="line.order_line_equipment" t-as="equipment">
                                                        <a t-att-href="('/web#id=%s&amp;view_type=form&amp;model=critt.equipment&amp;action=certification.act_equipement_view' % equipment.equipment_id.id)" t-esc="equipment.equipment_id.num_materiel"></a>
                                                        <br/>
                                                    </t>
                                                </t>
                                                <t t-else="">
                                                    <t t-foreach="line.order_line_equipment" t-as="equipment">
                                                        <a t-attf-href="/my/equipment/{{equipment.equipment_id.id}}">
                                                            <t t-esc="equipment.equipment_id.num_materiel"></t>
                                                         </a>
                                                        <br/>
                                                    </t>
                                                </t>
                                            </td>
                                            <td>
                                                <t t-esc="line.name"></t>
                                            </td>
                                            <td>
                                                <t t-esc="line.date_order" t-options='{"widget": "date"}'></t>
                                            </td>
                                            <td>
                                                <t t-esc="line.num_commande_client"></t>
                                            </td>
                                            <td t-if="origin == 'bo'">
                                                <t t-if="line.is_validate_by_customer">Oui</t>
                                                <t t-else="">Non</t>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </center>
                        </div>

                    <br/>
                    <!--<div class="col-lg-12">
                        <div class="col-xs-12">
                            <center>
                                <h4>
                                    <t>Planning</t>
                                </h4>
                                <div class="row">

                                    <div id="audits" style="display:none;">
                                         <div t-foreach="equipmentsWithAudit" t-as="audit">
                                             <span><t t-esc="audit.id"></t></span>
                                             <span><t t-esc="audit.date"></t></span>
                                             <span><t t-esc="audit.fin"></t></span>
